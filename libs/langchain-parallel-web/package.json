{
  "name": "langchain-parallel-web",
  "version": "0.0.1",
  "description": "ParallelWeb integration for LangChain.js",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:sanjeevhalyal/langchain-parallel-web.git"
  },
  "homepage": "https://github.com/sanjeevhalyal/langchain-parallel-web/tree/main/libs/langchain-parallel-web",
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "rm -rf dist/",
    "prepack": "npm run build",
    "test": "jest",
    "lint": "eslint --ext .ts src/",
    "lint:eslint": "NODE_OPTIONS=--max-old-space-size=4096 eslint --cache --ext .ts,.js src/",
    "lint:dpdm": "dpdm --exit-code circular:1 --no-warning --no-tree src/*.ts src/**/*.ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:dpdm",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch --testPathIgnorePatterns=\\.int\\.test.ts",
    "test:single": "NODE_OPTIONS=--experimental-vm-modules yarn run jest --config jest.config.cjs --testTimeout 100000",
    "test:int": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=\\.int\\.test.ts --testTimeout 100000 --maxWorkers=50%",
    "format": "prettier --config .prettierrc --write \"src\"",
    "format:check": "prettier --config .prettierrc --check \"src\""
  },
  "author": "LangChain",
  "license": "MIT",
  "peerDependencies": {
    "@langchain/core": ">=0.3.0 <0.4.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@swc/core": "^1.3.90",
    "@swc/jest": "^0.2.29",
    "@langchain/core": ">=0.3.72",
    "@tsconfig/recommended": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "dotenv": "^16.3.1",
    "dpdm": "^3.12.0",
    "release-it": "^15.10.1",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.6.4",
    "prettier": "^2.8.3",
    "rollup": "^4.5.2",
    "ts-jest": "^29.1.0",
    "typescript": "<5.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/"
  ]
}
